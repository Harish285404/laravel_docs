<!DOCTYPE html><html lang="en"><head>  <meta charset="UTF-8">  <meta name="viewport" content="width=device-width, initial-scale=1.0">  <title>S3 Upload Example</title></head><body><input type="file" id="fileInput" /><button onclick="uploadFile()">Upload File</button><script src="https://sdk.amazonaws.com/js/aws-sdk-2.1032.0.min.js"></script><script>  // Configure AWS SDK (exposing keys is insecure!)  AWS.config.update({    accessKeyId: 'AKIAZI2LDPUK6XIDJRHU', // EXPOSED (INSECURE)    secretAccessKey: 'esUUJ3tQ6OHMGw4g5QpBytUImbhd1M7FeXd/tjK9', // EXPOSED (INSECURE)    region: 'eu-north-1', // Your region  });  const s3 = new AWS.S3();  // Function to upload a file  function uploadFile() {    const fileInput = document.getElementById('fileInput');    const file = fileInput.files[0];    if (!file) {      alert('Please select a file to upload');      return;    }    const uploadParams = {      Bucket: 'baccvsbucket', // Your S3 bucket name      Key: `uploads/${Date.now()}_${file.name}`,      Body: file,      ContentType: file.type,    };    // Upload the file to S3    s3.upload(uploadParams, function (err, data) {      if (err) {        console.log('Error uploading file:', err);        alert('Upload failed');      } else {        console.log('File uploaded successfully:', data);        alert('File uploaded successfully');      }    });  }</script></body></html>